{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "src/**/*.ts", "src/**/*.vue"],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@variants/*": ["./src/variants/swb/*", "./src/variants/gbr/*"],
      "@/*": ["./src/*"]
    },
    "forceConsistentCasingInFileNames": true
  }
}
