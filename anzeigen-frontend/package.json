{
  "name": "anzeigen-portal",
  "version": "2.0.0",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=20 <=22"
  },
  "scripts": {
    "dev:swb": "vite --mode swb",
    "dev:gbr": "vite --mode gbr",
    "test": "vitest run",
    "build": "vite build --mode swb && vite build --mode gbr",
    "preview": "vite preview",
    "lint": "prettier . --check && eslint . && vue-tsc -p tsconfig.app.json --noEmit --skipLibCheck --composite false",
    "fix": "prettier . --write && eslint . --fix",
    "api-gen": "openapi-generator-cli generate"
  },
  "dependencies": {
    "@mdi/font": "7.4.47",
    "@muenchen/appswitcher-vue": "2.2.0",
    "@vueuse/core": "11.3.0",
    "@vueuse/math": "12.8.2",
    "@vueuse/router": "11.3.0",
    "pinia": "2.3.1",
    "roboto-fontface": "0.10.0",
    "vue": "3.5.25",
    "vue-i18n": "11.1.12",
    "vue-router": "4.5.1",
    "vuetify": "3.11.3",
    "axios": "1.9.0"
  },
  "devDependencies": {
    "@muenchen/prettier-codeformat": "1.0.2",
    "@openapitools/openapi-generator-cli": "2.20.6",
    "@tsconfig/node-lts": "22.0.4",
    "@types/node": "22.14.1",
    "@vitejs/plugin-vue": "5.2.4",
    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/eslint-config-typescript": "14.5.1",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.7.0",
    "eslint": "9.28.0",
    "eslint-plugin-vue": "9.33.0",
    "jsdom": "25.0.1",
    "prettier": "3.5.3",
    "typescript": "5.9.3",
    "unplugin-fonts": "1.4.0",
    "vite": "5.4.21",
    "vite-plugin-vue-devtools": "7.7.9",
    "vite-plugin-vuetify": "2.1.2",
    "vitest": "2.1.9",
    "vue-tsc": "2.2.12"
  }
}
